{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "build",
    "moduleResolution": "node",
    "target": "es2018",
    "module": "commonjs",
    "sourceMap": true,
    "lib": ["es6", "esnext.asynciterable"],
    "baseUrl": "./src",
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types"],
    "jsx": "react-jsx"
  },
  "include": ["src", "../frontend/src/views"],
  "typeAcquisition": {
    "enable": true
  }
}
