{
  "compileOnSave": true,
  "compilerOptions": {
    "jsx": "react",
    "outDir": "build",
    "moduleResolution": "node",
    "target": "es2018",
    "module": "commonjs",
    "sourceMap": true,
    "lib": ["es6", "esnext.asynciterable"],
    "baseUrl": "./src",
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["src"],
  "typeAcquisition": {
    "enable": true
  }
}
